---
interface Props {
	class?: string;
}

const { class: className } = Astro.props;
---

<a  class:list={['logo', className]} href="/" title="AWSM Docs">AWSM</a>

<style>
  .logo {
    font-size: var(--awsm-font-size-xxl);
    font-weight: bold;
    color: var(--awsm-color-primary-shade);
    text-decoration: none;
  }
</style>
